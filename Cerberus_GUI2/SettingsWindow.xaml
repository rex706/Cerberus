<Window x:Class="Cerberus_GUI2.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cerberus_GUI2"
        mc:Ignorable="d"
        Title="Settings" Height="428" Width="528" Background="#FF333638" ResizeMode="CanMinimize">
    <Grid>
        <ListBox x:Name="ChannelsBox" HorizontalAlignment="Left" Height="99" Margin="10,31,0,0" VerticalAlignment="Top" Width="198" Background="#FF111111" Foreground="#FFD1D1D1"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Text Channels" VerticalAlignment="Top" Foreground="White" Width="198" TextAlignment="Center"/>
        <ListBox x:Name="ChannelsBox_Copy" HorizontalAlignment="Left" Height="99" Margin="213,31,0,0" VerticalAlignment="Top" Width="198" Background="#FF111111" Foreground="#FFD1D1D1"/>
        <TextBlock HorizontalAlignment="Left" Margin="213,10,0,0" TextWrapping="Wrap" Text="Bypass Safe Search" VerticalAlignment="Top" Foreground="White" Width="198" TextAlignment="Center"/>

    </Grid>
</Window>
